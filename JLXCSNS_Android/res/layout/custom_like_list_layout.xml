<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.jlxc.app"
    android:id="@+id/layout_like_list_root_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="@dimen/news_item_margins_right" >

    <!-- 赞的icon -->

    <ImageView
        android:id="@+id/tv_like_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:src="@drawable/like_icon" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewA"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/tv_like_icon"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewB"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/imageViewA"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewC"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/imageViewB"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewD"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/imageViewC"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewE"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/imageViewD"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewF"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/imageViewE"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewG"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/imageViewF"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewH"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/imageViewG"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewI"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/imageViewH"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewJ"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/imageViewI"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <com.jlxc.app.base.ui.view.RoundImageView
        android:id="@+id/imageViewK"
        android:layout_width="@dimen/head_image_size_small"
        android:layout_height="@dimen/head_image_size_small"
        android:layout_centerInParent="true"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/imageViewJ"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_launcher"
        app:rect_adius="1dp" />

    <TextView
        android:id="@+id/tv_like_all_person"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerInParent="true"
        android:background="@drawable/all_like_person"
        android:gravity="center"
        android:text="22" />

</RelativeLayout>